<OntologyConfiguration>
        <MainOntology>
                <URL>http://www.owl-ontologies.com/OntologyAnalysis.owl</URL>
                <LocalCopy>ontology_analysis.owl</LocalCopy>
        </MainOntology>   
        
        <!-- There are not subontologies -->
        
        <CaseMainConcept>Analysis</CaseMainConcept>
        
        <DescriptionClassName>pm.web.cbr.experiment.Description</DescriptionClassName>
        <DescriptionMappings>
                <Map>
                        <Property>hasExperience</Property>
                        <Concept>Experience</Concept>
                        <Attribute>experience</Attribute>
                </Map>	
                <Map>
                        <Property>hasWorker</Property>
                        <Concept>Worker</Concept>
                        <Attribute>worker</Attribute>
                </Map>	
                <Map>
                        <Property>hasIndividualPlanSum</Property>
                        <Concept>IndividualPlanSum</Concept>
                        <Attribute>individualPlanSum</Attribute>
                </Map>
                <Map>
                        <Property>hasIndividualPlanDone</Property>
                        <Concept>IndividualPlanDone</Concept>
                        <Attribute>individualPlanDone</Attribute>
                </Map>
                <Map>
                        <Property>hasIndividualPlan</Property>
                        <Concept>IndividualPlan</Concept>
                        <Attribute>individualPlan</Attribute>
                </Map>
                <Map>
                        <Property>hasReceiptSum</Property>
                        <Concept>ReceiptSum</Concept>
                        <Attribute>receiptSum</Attribute>
                </Map>
                <Map>
                        <Property>hasReceiptNum</Property>
                        <Concept>ReceiptNum</Concept>
                        <Attribute>receiptNum</Attribute>
                </Map>
                <Map>
                        <Property>hasReceiptQuantity</Property>
                        <Concept>ReceiptQuantity</Concept>
                        <Attribute>receiptQuantity</Attribute>
                </Map>
                <Map>
                        <Property>hasPeriod</Property>
                        <Concept>Period</Concept>
                        <Attribute>period</Attribute>
                </Map>
                <Map>
                        <Property>hasIndividualPlanMin</Property>
                        <Concept>IndividualPlanMin</Concept>
                        <Attribute>individualPlan_min</Attribute>
                </Map>
                <Map>
                        <Property>hasReceiptSumMin</Property>
                        <Concept>ReceiptSumMin</Concept>
                        <Attribute>receiptSum_min</Attribute>
                </Map>
                <Map>
                        <Property>hasReceiptQuantityMin</Property>
                        <Concept>ReceiptQuantityMin</Concept>
                        <Attribute>receiptQuantity_min</Attribute>
                </Map>
        </DescriptionMappings>
        
        <SolutionClassName>pm.web.cbr.experiment.Solution</SolutionClassName>
        <SolutionMappings>
                <Map>
                        <Property>hasRecommendation</Property>
                        <Concept>Recommendation</Concept>
                        <Attribute>recommendation</Attribute>
                </Map>
        </SolutionMappings>
 </OntologyConfiguration>